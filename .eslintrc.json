{
  "extends": [
    "angular",
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "angular/log": "off",
    "no-new-wrappers": "off",
    "no-useless-escape": "off",
    "no-new-object": "off",
    "no-loop-func": "off",
    "no-debugger": "off",
    "max-params": "off",
    "object-curly-spacing": "error",
    "no-console": "off"
  },
  "settings": {
    "import/resolver": "babel-root-import"
  },
  "globals": {
    "require": true,
    "console": true
  }
}
